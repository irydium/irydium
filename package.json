{
  "name": "@irydium/project",
  "version": "0.0.1",
  "description": "Irydium project monorepo",
  "main": "index.js",
  "browserslist": "> 0.25%, not dead",
  "engines": {
    "node": ">=14"
  },
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "dev": "lerna run --stream --scope @irydium/viewer dev --",
    "dev-site": "lerna run --stream --scope irydium-site dev",
    "build": "lerna run build",
    "compile": "lerna run --loglevel error --scope @irydium/compiler compile --",
    "heroku-postbuild": "lerna bootstrap && lerna run build",
    "start": "cd packages/site && node __sapper__/build",
    "test": "jest",
    "format": "prettier --plugin-search-dir=. --write .",
    "format:check": "prettier --plugin-search-dir=. --check ."
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",

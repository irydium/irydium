{
  "name": "@irydium/project",
  "version": "0.0.1",
  "description": "Irydium project monorepo",
  "main": "index.js",
  "browserslist": "> 0.25%, not dead",
  "engines": {
    "node": ">=14"
  },
  "private": true,
  "scripts": {
    "compile": "cd packages/compiler && npm run compile --",
    "dev": "cd packages/viewer && pnpm run dev --",
    "dev-compiler": "cd packages/compiler && pnpm run dev",
    "dev-site": "cd packages/site && pnpm run dev",
    "build-site": "pnpm build --filter=\"@irydium/compiler\" --filter=\"@irydium/site\"",
    "lint": "eslint './packages/**/src/**/*.{js,ts,svelte}'",
    "test": "jest",
    "format": "prettier --plugin-search-dir=. --write .",
    "format:check": "prettier --plugin-search-dir=. --check .",
		"release": "pnpm -r build && changeset publish"
  },

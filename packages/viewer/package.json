{
  "name": "@irydium/viewer",
  "version": "0.0.6",
  "description": "Live viewer for irydium files",
  "author": "William Lachance <wlach@protonmail.com>",
  "homepage": "https://github.com/irydium/irydium/tree/master/packages/viewer#readme",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "src/viewer.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "bin": {
    "irmd-viewer": "bin/irmd-compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/irydium/irydium.git"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup --config --watch",
    "start": "bin/irmd-viewer"
  },
  "bugs": {
    "url": "https://github.com/irydium/irydium/issues"
  },
  "dependencies": {
    "@irydium/compiler": "file:../compiler",
    "livereload": "^0.9.1",
    "polka": "next"
  }
}
